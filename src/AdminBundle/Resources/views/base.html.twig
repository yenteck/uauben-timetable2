{% block notice %}
    <div class="modal fade" id="notice">
        <div class="modal-dialog" role="document">

            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                        <span class="sr-only">Close</span>
                    </button>
                    <h4 class="modal-title text-center"><b><span class="glyphicon glyphicon-exclamation-sign"></span> INFOS</b></h4>
                </div>
                <div class="modal-body" id="load">
                    {% set hasFlash = false %}
                    {% for notice in app.session.flashbag.get('notice') %}

                        <div class="alert alert-info text-center">
                           <b> {{ notice }}</b>
                        </div>
                        {% set hasFlash = true %}
                    {%  endfor %}
                </div>


            </div><!-- /.modal-content -->
        </div>
    </div>

    <script>


        {% if hasFlash %}
        $(window).on("load",function () {
            $("#notice").modal()
        })
        {% endif %}

    </script>
{% endblock notice %}
